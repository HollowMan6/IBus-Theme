msgid " Tool Author:"
msgstr " 工具作者："

#: ibus_theme_tools.py:178 ibus_theme_tools.py:330
msgid " Tool Source Code:"
msgstr " 工具源代码："

#: ibus_theme_tools.py:179 ibus_theme_tools.py:331
msgid " Tool Licence:"
msgstr " 工具许可证："

